<UserControl x:Class="PointOfSale.Entree.cPhilly"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:PointOfSale.Entree"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <ListBox>
            <CheckBox>Sirloin</CheckBox>
            <CheckBox>Onion</CheckBox>
            <CheckBox>Roll</CheckBox>
        </ListBox>
        <Button Click="doneButton_Click" Name="DoneButton" Margin="200,200,200,200">Done</Button>
    </Grid>
</UserControl>
